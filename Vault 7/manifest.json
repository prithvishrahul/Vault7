{
    "manifest_version": 2,
    "name":"Vault 7 password manager",
    "description":"Password manager built using node.js as chrome extension",
    "version": "0.01",
    "content_scripts": [
        {
          "matches": ["https://*/*"],
          "js": ["js/lib/jquery-3.5.1.min.js","js/app/content.js"]
        }
      ],
    "background":{
        "scripts":["js/lib/jquery-3.5.1.min.js","js/lib/buffer.js","js/lib/otplib.js","js/app/background.js"]
        
    },
    "browser_action":{
        "default_popup":"views/popup.html"
    }
    
}